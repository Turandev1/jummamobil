{
  "expo": {
    "name": "Jumma",
    "slug": "jumma",
    "version": "1.0.6",
    "orientation": "portrait",
    "icon": "./assets/mescid.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/mescid.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyB-zf2WXmUBDb_j6By2jGA39cRlVzJt7Fk"
        }
      },
      "versionCode": 3,
      "permissions": [
        "RECEIVE_BOOT_COMPLETED",
        "WAKE_LOCK",
        "NOTIFICATIONS",
        "android.permission.INTERNET",
        "android.permission.WAKE_LOCK",
        "com.google.android.c2dm.permission.RECEIVE"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/mescid.png",
        "backgroundColor": "#ffffff"
      },
      "googleServicesFile": "./google-services.json",
      "edgeToEdgeEnabled": true,
      "package": "com.JummaMarket"
    },
    "web": {
      "favicon": "./assets/jummaicon.png",
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "bbc5b722-ca06-4224-a998-f8191c2c8b10"
      }
    },
    "plugins": [
      "expo-video",
      "expo-secure-store",
      "expo-build-properties",
      "expo-localization",
      [
        "expo-notifications",
        {
          "icon": "./assets/mescid.png"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      "expo-audio"
    ]
  }
}
